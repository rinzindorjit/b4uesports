{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/users",
      "destination": "/api/users.ts"
    },
    {
      "source": "/api/packages",
      "destination": "/api/packages.ts"
    },
    {
      "source": "/api/pi-price",
      "destination": "/api/pi-price.ts"
    },
    {
      "source": "/api/payments",
      "destination": "/api/payments.ts"
    },
    {
      "source": "/api/transactions",
      "destination": "/api/transactions.ts"
    },
    {
      "source": "/api/admin",
      "destination": "/api/admin.ts"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.ts"
    }
  ]
}