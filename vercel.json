{
  "version": 2,
  "buildCommand": "npm run build-deploy",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "cleanUrls": true
}