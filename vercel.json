{
  "version": 2,
  "buildCommand": "npm run build-deploy",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/pi-price", "destination": "/api/pi-price.ts" },
    { "source": "/api/users", "destination": "/api/users.ts" },
    { "source": "/api/packages", "destination": "/api/packages.ts" },
    { "source": "/api/payments", "destination": "/api/payments.ts" },
    { "source": "/api/transactions", "destination": "/api/transactions.ts" },
    { "source": "/api/health", "destination": "/api/health.ts" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}