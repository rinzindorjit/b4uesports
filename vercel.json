{
  "version": 2,
  "buildCommand": "node build.cjs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "cleanUrls": true
}