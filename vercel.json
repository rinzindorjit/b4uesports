{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-handler.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "FRONTEND_URL": "https://b4uesports.vercel.app",
    "API_URL": "https://b4uesports.vercel.app"
  },
  "functions": {
    "server/vercel-handler.ts": {
      "runtime": "@vercel/node@2.0.0",
      "includeFiles": "dist/**,server/**,api/**"
    }
  }
}