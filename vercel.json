{
  "version": 2,
  "buildCommand": "node build.cjs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@4.0.0",
      "includeFiles": "api/dist_server/**"
    }
  },
  "cleanUrls": true
}