{
  "version": 2,
  "buildCommand": "node build.cjs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "dist/api/**/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}