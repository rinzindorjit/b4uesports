{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "/api/users.ts"
    },
    {
      "src": "/api/packages",
      "dest": "/api/packages.ts"
    },
    {
      "src": "/api/pi-price",
      "dest": "/api/pi-price.ts"
    },
    {
      "src": "/api/payments",
      "dest": "/api/payments.ts"
    },
    {
      "src": "/api/transactions",
      "dest": "/api/transactions.ts"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.ts"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ]
}